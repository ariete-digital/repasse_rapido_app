{
  "cli": {
    "version": ">= 5.9.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "autoIncrement": true,
      "cache": {
        "disabled": true
      },
      "env": {
        "NODE_ENV": "production",
        "API_URL_PROD": "https://api-repasses.arietedigital.com.br/api",
        "EXPO_NO_DEV": "true",
        "EXPO_OFFLINE": "true"
      }
    },
    "production": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "API_URL_PROD": "https://api-repasses.arietedigital.com.br/api"
      }
    }
  },
  "submit": {
    "preview": {},
    "production": {}
  }
}
